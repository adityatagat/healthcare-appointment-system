{
  "name": "appointment-service",
  "version": "1.0.0",
  "description": "Healthcare Appointment Management Microservice",
  "main": "dist/index.js",
  "scripts": {
    "start": "node -r module-alias/register dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only -r module-alias/register src/index.ts",
    "build": "tsc && cp -r src/config dist/",
    "test": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "docker:build": "docker build -t appointment-service .",
    "docker:run": "docker run -p 3001:3001 appointment-service"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.8.1",
    "@types/express-mongo-sanitize": "^1.3.2",
    "@types/express-rate-limit": "^5.1.3",
    "@types/hpp": "^0.2.6",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mongodb": "^4.0.7",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^18.0.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "http-status-codes": "^2.2.0",
    "joi": "^17.9.2",
    "module-alias": "^2.2.3",
    "mongoose": "^7.3.1",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typescript": "^5.0.4",
    "winston": "^3.8.2",
    "xss-clean": "^0.1.4"
  },
  "_moduleAliases": {
    "@": "./dist",
    "@config": "./dist/config",
    "@controllers": "./dist/controllers",
    "@middleware": "./dist/middleware",
    "@models": "./dist/models",
    "@routes": "./dist/routes",
    "@services": "./dist/services",
    "@utils": "./dist/utils"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.4",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "jsonwebtoken": "^9.0.2",
    "prettier": "^2.8.7",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
